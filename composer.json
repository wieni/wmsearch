{
    "name": "wieni/wmsearch",
    "type": "drupal-module",
    "description": "Elasticsearch api",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Wieni",
            "email": "info@wieni.be"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "drupal/core": "^8.5",
        "guzzlehttp/guzzle": "^6.1"
    },
    "suggest": {
        "drupal/queue_ui": "to be able to fill and process the queue through the admin interface"
    },
    "extra": {
        "drush": {
            "services": {
                "drush.services.yml": "^9"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
