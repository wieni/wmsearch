{
    "name": "wieni/wmsearch",
    "description": "Elasticsearch api",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Wieni",
            "email": "info@wieni.be"
        }
    ],
    "repositories": {
        "drupal-composer": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "drupal-module",
    "require": {
        "php": ">=7.1",
        "drupal/core": "^8.8 || ^9",
        "guzzlehttp/guzzle": "^6.1"
    },
    "extra": {
        "drush": {
            "services": {
                "drush.services.yml": "^9"
            }
        }
    },
    "suggest": {
        "drupal/queue_ui": "to be able to fill and process the queue through the admin interface"
    }
}
